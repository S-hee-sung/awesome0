<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oriental.bitcoin.mapper.AccountMapper">
<insert id="accountRegDoIt" parameterType="com.oriental.bitcoin.model.AccountDTO">
    insert into account values(#{ac_id},#{ac_pw},#{ac_name},#{ac_birth},#{ac_phone},#{ac_gender},sysdate)
</insert>

    <select id="getAccountById"
            parameterType="String"
            resultType="com.oriental.bitcoin.model.AccountDTO">
        select *
        from account
        where ac_id = #{ac_id}
    </select>

</mapper>